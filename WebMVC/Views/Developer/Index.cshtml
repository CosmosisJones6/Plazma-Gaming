@model IEnumerable<WebMVC.Models.Developer>
@{
    ViewData["Title"] = "Developers";
}

<h1 style="padding-top:150px; text-align:center;">Developers working with us</h1>
<div class="picture-text-list">
    @foreach(var item in Model)
			{
                <div class="picture-text">
                    <img src="/media/placeholder.jpg" />
                    <div id="text">
                        <h2>@Html.DisplayFor(x => item.Name)</h2>
                        <p>@Html.DisplayFor(x => item.Description)</p>
                    </div>
                </div>
			}
</div>